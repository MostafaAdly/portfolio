<html lang="en">
    <head>
        <title>Register - Classroom</title>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />

        <link rel="icon" type="image/png" href="/img/cic-logo.jpg" />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/vender/bootstrap/css/bootstrap.min.css"
        />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/fonts/font-awesome-4.7.0/css/font-awesome.min.css"
        />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/fonts/Linearicons-Free-v1.0.0/icon-font.min.css"
        />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/vender/animate/animate.css"
        />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/vender/css-hamburgers/hamburgers.min.css"
        />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/vender/animsition/css/animsition.min.css"
        />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/vender/select2/select2.min.css"
        />
        <link
            rel="stylesheet"
            type="text/css"
            href="/auth/vender/daterangepicker/daterangepicker.css"
        />
        <link rel="stylesheet" type="text/css" href="/auth/css/util.css" />
        <link rel="stylesheet" type="text/css" href="/auth/css/main.css" />
        <script
            src="https://kit.fontawesome.com/e6c13277a1.js"
            crossorigin="anonymous"
        ></script>
        <meta name="robots" content="noindex, follow" />
    </head>
    <body>
        <div class="limiter">
            <div class="container-login100">
                <div class="wrap-login100 p-l-85 p-r-85 p-t-55 p-b-55">
                    <form
                        class="login100-form validate-form flex-sb flex-w"
                        action="/auth"
                        method="post"
                    >
                        <span
                            class="login100-form-title p-b-32"
                            style="color: rgb(148, 49, 49);text-align:center"
                        >
                            Account Register
                        </span>

                        <span class="txt1 p-b-11">
                            Username
                        </span>
                        <div
                            class="wrap-input100 validate-input m-b-36"
                            data-validate="Username is required"
                        >
                            <input
                                class="input100"
                                type="text"
                                name="username"
                            />
                            <span class="focus-input100"></span>
                        </div>

                        <span class="txt1 p-b-11">
                            Email
                        </span>
                        <div
                            class="wrap-input100 validate-input m-b-36"
                            data-validate="Email is required"
                        >
                            <input class="input100" type="text" name="email" />
                            <span class="focus-input100"></span>
                        </div>

                        <span class="txt1 p-b-11">
                            Password
                        </span>
                        <div
                            class="wrap-input100 validate-input m-b-12"
                            data-validate="Password is required"
                        >
                            <span class="btn-show-pass">
                                <i
                                    class="fa-solid fa-eye"
                                    onclick="document.getElementById('password').type = document.getElementById('password').type == 'password' ? 'text' : 'password'"
                                ></i>
                            </span>
                            <input
                                class="input100"
                                type="password"
                                name="password"
                                id="password"
                            />
                            <span class="focus-input100"></span>
                        </div>

                        <div class="flex-sb-m w-full p-b-48">
                            <div class="contact100-form-checkbox" hidden>
                                <input
                                    class="input-checkbox100"
                                    id="ckb1"
                                    type="checkbox"
                                    name="remember-me"
                                />
                                <label class="label-checkbox100" for="ckb1">
                                    Remember me
                                </label>
                            </div>
                            <div>
                                <a href="{{homePage}}/login" class="txt3">
                                    Already have an account? Sign in
                                </a>
                            </div>
                        </div>
                        <div class="container-login100-form-btn">
                            <button
                                class="login100-form-btn"
                                style="background-color: rgb(148, 49, 49);width:50%;margin-left:25%;margin-right:25%;"
                                type="submit"
                            >
                                Login
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div id="dropDownSelect1"></div>
        <script src="/auth/vender/jquery/jquery-3.2.1.min.js"></script>
        <script src="/auth/vender/animsition/js/animsition.min.js"></script>
        <script src="/auth/vender/bootstrap/js/popper.js"></script>
        <script src="/auth/vender/bootstrap/js/bootstrap.min.js"></script>
        <script src="/auth/vender/select2/select2.min.js"></script>
        <script src="/auth/vender/daterangepicker/moment.min.js"></script>
        <script src="/auth/vender/daterangepicker/daterangepicker.js"></script>
        <script src="/auth/vender/countdowntime/countdowntime.js"></script>
        <script src="/auth/js/main.js"></script>
